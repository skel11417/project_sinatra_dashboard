<table class="results-table" border="visible"
<tr>
  <th>Title</th>
  <th>Company</th>
  <th>Link</th>
  <th>Location</th>
  <th>Date</th>
  <th>Company ID</th>
  <th>Job ID</th>
</tr>
<% arr_of_arrs = results %>
<% arr_of_arrs.each do |job| %>
  <tr>
  <% job.each do |elem| %>
    <% format_link(elem) %>
    <tl><%=elem%></tl>
  <% end %>
<% end %>
</table>
